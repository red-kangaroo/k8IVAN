// worldmap points of interest
owterrain
{
  /* Obligatory: BitmapPos */
  NameSingular = "<unknown>";
  NameStem = "";
  IsAbstract = true; /* This is false by default and does not inherit! */
  UsesLongArticle = false;
  AttachedArea = 0;
  CanBeGenerated = false; // "Enabled"
  NativeGTerrainType = 0;
  RevealEnvironmentInitially = false;
  PlaceInitially = true; // POI can be placed, but not revealed
  CanBeOnAnyTerrain = false;
  WantContinentWith = 0; // WARNING: POIs will be created in the order of declaration!
  Probability = 100; // in percents
  CanBeSkipped = false; // if it is impossible to generate such POI, skip it
}
